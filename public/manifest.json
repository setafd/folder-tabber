{
  "manifest_version": 3,
  "name": "FolderTabber",
  "version": "0.3.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["bookmarks", "favicon", "activeTab", "tabGroups", "storage"],
  "web_accessible_resources": [
    {
      "resources": ["_favicon/*"],
      "matches": ["<all_urls>"],
      "extension_ids": ["*"]
    }
  ]
}
